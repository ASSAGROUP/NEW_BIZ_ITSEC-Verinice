proc "gen-rep" [val report] [val qualifier "ISM-ISO"] [val suffix ".pdf"]{
	try {
	    start-time-measure $report
		gen-rep-com $report "ISM" "importierte Objekte/[Mein Unternehmen1] [5a79b4]" $qualifier $suffix
		stop-time-measure $report | write-lines [concat "file:" $basedir "/report-measure.csv"] true
	} -catch {
		log [concat "ERROR creating report " $report]
		try {
			get-window "Fehler" | get-button OK | click
		} -catch {}
	}
	test-for-report-file "iso-reports" $report $qualifier $suffix
}

log "Start report testing ISM-ISO"
enable-reporting-log

ism-import-vna "reportTestData1.vna"

gen-rep "(S) ISM-ISO: Erkl√§rung zur Anwendbarkeit"
gen-rep "(S) ISM-ISO: Export: Aufgaben (de)" "ISM-ISO" ".xls"
gen-rep "(S) ISM-ISO: Export: Aufgaben (en)" "ISM-ISO" ".xls"

delete-iso-model "importierte Objekte/[Mein Unternehmen1] [5a79b4]"
test-for-report-errors "iso-reports"

